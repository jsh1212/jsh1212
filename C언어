#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

#define VALUE 10000  

void square(int n) {
    if (n <= 0) return;  
    
    for (int i = 0; i < n; i++) {
        printf("*");
    }
    printf("\n");

    
    for (int i = 0; i < n - 2; i++) {
        printf("*");
        for (int j = 0; j < n - 2; j++) {
            printf(" ");
        }
        printf("*\n");
    }


    if (n > 1) {
        for (int i = 0; i < n; i++) {
            printf("*");
        }
        printf("\n");
    }
}

int main() {
    int n;

    scanf("%d", &n);

     square(n);

    return 0;
}


